<root options='{alignment: "middle-center"}'>
	<row width=500 height=370 justifyContent='JUSTIFY_FLEX_START' alignItems='ALIGN_CENTER' flexDirection='FLEX_DIRECTION_COLUMN_REVERSE' style='asset: "Background";'>
		<row width=500 height=50 justifyContent='JUSTIFY_CENTER' alignItems='ALIGN_CENTER' flexDirection='FLEX_DIRECTION_ROW'>
			<Button id='list' width=50 height=50 margins=6 style='asset: "List";'></Button>
			<Button id='video' width=50 height=50 margins=6 style='asset: "Movie";'></Button>
			<row width=290 height=30 margins=5 justifyContent='JUSTIFY_CENTER' alignItems='ALIGN_CENTER' flexDirection='FLEX_DIRECTION_ROW' style='asset: "Input1";'>
				<Input id='search_text' width=250 height=30 options='{textHeight: 0.15, textAnchor: "middle-center"}'></Input>
				<Button id='search_button' width=25 height=25 style='asset: "Search";'></Button>
			</row>
			<Button id='settings' width=50 height=50 margins=6 style='asset: "Settings";'></Button>
		</row>
		<row id='main' width=500 height=320 justifyContent='JUSTIFY_CENTER' alignItems='ALIGN_CENTER' flexDirection='FLEX_DIRECTION_ROW'></row>
		<row id='logo' positionType='POSITION_TYPE_ABSOLUTE' y=60 width=300 height=300 style='asset: "Logo";'></row>
	</row>
</root>