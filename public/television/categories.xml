<row id='categories' width=500 height=320 justifyContent='JUSTIFY_CENTER' alignItems='ALIGN_CENTER' flexDirection='FLEX_DIRECTION_COLUMN_REVERSE'>
	<row width=500 height=300 justifyContent='JUSTIFY_CENTER' alignItems='ALIGN_CENTER' flexDirection='FLEX_DIRECTION_ROW'>
		<Button width=20 height=20 id='prev' options='{textHeight: 0.08, textAnchor: "middle-center"}' style='asset: "Left";'></Button>
		<Grid id='categories_list' justifyContent='JUSTIFY_CENTER' alignItems='ALIGN_CENTER' flexDirection='FLEX_DIRECTION_COLUMN_REVERSE' options='{row:3, col:4, width:95, height:85, margins:8, template_url: "television/category_cell.xml"}'></Grid>
		<Button width=20 height=20 id='next' options='{textHeight: 0.08, textAnchor: "middle-center"}' style='asset: "Right";'></Button>
	</row>
	<Pager id='categories_pager' width=20 height=20 justifyContent='JUSTIFY_CENTER' alignItems='ALIGN_CENTER' flexDirection='FLEX_DIRECTION_ROW'>
		<Button width=20 height=20 id='num' options='{textHeight: 0.08, textAnchor: "middle-center"}'></Button>
	</Pager>
</row>